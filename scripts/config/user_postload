flexus.set "-L1d:level"                   "eL1"                # (CacheLevel) CacheLevel
flexus.set "-L1d:mt_width"                "1"                  # (MTWidth) Number of threads sharing this cache
flexus.set "-L1d:notify_reads"            "0"                  # (NotifyReads) Notify on reads (does not notify on fast-hit)
flexus.set "-L1d:notify_writes"           "0"                  # (NotifyWrites) Notify on writes
flexus.set "-L1d:protocol"                "InclusiveMESI"      # (Protocol) Name of the coherence protocol (InclusiveMESI)
flexus.set "-L1d:rsize"                   "1024"               # (RegionSize) Region size in bytes
flexus.set "-L1d:rt_assoc"                "16"                 # (RTAssoc) RegionTracker Associativity
