flexus.set "-L1d:rt_size"                 "8192"               # (RTSize) RegionTracker size (number of regions tracked)
flexus.set "-L1d:size"                    "32768"              # (Size) Cache size in bytes
flexus.set "-L1d:skew_block_set"          "0"                  # (SkewBlockSet) skew block set indices based on rt way
flexus.set "-L1d:snoop_lru"               "0"                  # (SnoopLRU) Move block to LRU position when a Snoop (ReturnReq) is recieved for a block in Modified or Exclusive state
